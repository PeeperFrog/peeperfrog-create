  "size_mapping": {
    "_note": "Maps image_size parameter to provider-specific resolution/cost keys",
    "gemini": {
      "small": "1K",
      "medium": "2K",
      "large": "2K",
      "xlarge": "4K"
    },
    "openai": {
      "_note": "OpenAI resolution depends on aspect_ratio, not image_size",
      "1:1": "1024x1024",
      "16:9": "1536x1024",
      "9:16": "1024x1536",
      "4:3": "1536x1024",
      "3:4": "1024x1536"
    },
    "together": {
      "_note": "Together resolution depends on both aspect_ratio and image_size. Cost = width * height * per_megapixel_cost / 1000000"
    }
  },

  "cost_estimation_formula": {
    "gemini_pro": "per_image_cost[size] + text_input_per_image_estimate + (num_reference_images * reference_image_cost_each) + (search_grounding_cost_per_query if grounding) + (thinking_token_overhead[level] if thinking)",
    "gemini_fast": "per_image_cost['1K'] + text_input_per_image_estimate",
    "gemini_fast_batch": "batch_per_image_cost['1K'] + text_input_per_image_estimate",
    "openai": "per_image_cost[quality][resolution] + text_input_per_image_estimate",
    "together": "(width * height / 1000000) * per_megapixel_cost"
  },

